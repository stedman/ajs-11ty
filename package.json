  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run css-build && eleventy",
    "clean": "rm -rf ./_site",
    "css-build": "node-sass _sass/main.scss assets/css/main.css --output-style compressed",
    "css-dev": "node-sass _sass/main.scss assets/css/main.css",
    "css-watch": "npm run css-dev -- --watch",
    "deploy": "npm run build && gh-pages -d _site",
    "start": "eleventy --serve --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
